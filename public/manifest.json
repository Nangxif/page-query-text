{
  "name": "文本检索",
  "description": "这是一款全能的文本检索工具",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-scripts/index.js"]
    }
  ],
  "permissions": ["sidePanel"],
  "side_panel": {
    "default_path": "Sidebar.html",
    "default_title": "打开侧边栏"
  },
  "icons": {}
}
